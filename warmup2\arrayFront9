public boolean arrayFront9(int[] nums) {
  int h=0;
  int n=nums.length;
  if (n>=4) n=4;
  boolean flag=false;
  while(h<n)
  {
    if(nums[h]==9)
    {
      flag=true;
      break;
    
    }
    else
    { 
      if(h>=n) 
      {
        flag=false;
      }
      else
      {
        h+=1;
      }
    }
    
  }
  if(flag==true) return true;
  else return false;

}
